_viv_uniform VXC_512Bits uniF16MulF16_2x8;

#define PRELU_AXIS0_I8F16TOI8_PROCESS(read_fun, write_fun) \
    read_fun(src0, input, coord, 0, VXC_MODIFIER(0, 15, 0, VXC_RM_TowardZero, 0)); \
    VXC_ReadImage(vec_in0, param, coord_p, VXC_5BITOFFSET_XY(0, 0), VXC_MODIFIER(0, 7, 0, VXC_RM_TowardZero, 0)); \
    VXC_ReadImage(vec_in1, param, coord_p, VXC_5BITOFFSET_XY(8, 0), VXC_MODIFIER(0, 7, 0, VXC_RM_TowardZero, 0)); \
    VXC_DP2x8(src2, src0, src0, VXC_MODIFIER(0, 7, 0, VXC_RM_TowardZero, 0), uniPreluI8toF16Lo_2x8); \
    VXC_DP2x8(src3, src0, src0, VXC_MODIFIER(0, 7, 0, VXC_RM_TowardZero, 0), uniPreluI8toF16Hi_2x8); \
    vec0 = vec_in0; \
    _viv_asm(COPY, vec1, src2, 16); \
    vec2 = vec1 >= 0 ? const1 : vec0; \
    _viv_asm(COPY, param_h, vec2, 16); \
    VXC_DP2x8(dst, src2, param_h, VXC_MODIFIER(0, 7, 0, VXC_RM_ToNearestEven, 1), uniF16MulF16_2x8); \
    vec0 = vec_in1; \
    _viv_asm(COPY, vec1, src3, 16); \
    vec2 = vec1 >= 0 ? const1 : vec0; \
    _viv_asm(COPY, param_h, vec2, 16); \
    VXC_DP2x8(dst, src3, param_h, VXC_MODIFIER(8, 15, 0, VXC_RM_ToNearestEven, 1), uniF16MulF16_2x8); \
    write_fun(output, coord, dst, VXC_MODIFIER(0, 15, 0, VXC_RM_TowardZero, 0));

